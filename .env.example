# Exemplos de variáveis locais (não sensíveis) – ajustar conforme necessidade local
# Produção: segredos via OIDC/Key Vault no CI/CD, não versionar .env

# Database
conexao_de_sorte_database_url=
conexao_de_sorte_database_jdbc_url=
conexao_de_sorte_database_r2dbc_url=
conexao_de_sorte_database_username=
conexao_de_sorte_database_password=

# Redis
conexao_de_sorte_redis_host=
conexao_de_sorte_redis_port=
conexao_de_sorte_redis_password=
conexao_de_sorte_redis_database=

# JWT (apenas valores públicos/local)
conexao_de_sorte_jwt_issuer=
conexao_de_sorte_jwt_jwks_uri=

# CORS
conexao_de_sorte_cors_allowed_origins=http://localhost:3000
conexao_de_sorte_cors_allow_credentials=false

# SSL (local/dev)
conexao_de_sorte_ssl_enabled=false
conexao_de_sorte_ssl_keystore_path=
conexao_de_sorte_ssl_keystore_password=
